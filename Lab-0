#include <stdio.h>

void max1(int a[],int n)
	{ 	
	int max=a[0];
	for(int i=0;i<n;i++)
	{
		if(a[i]>max)
		{
			max=a[i];
			
		}
		
	}
	printf("Max=%d",max);
	
	}

int main(int argc, char **argv)
{
	int i,n,a[10];
	printf("Enter the value of n\n");
	scanf("%d",&n);
	printf("Enter %d numbers\n",n);
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	max1(a,n);
	
  
  
  }
